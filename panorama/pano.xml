	<include url="plugins/swipe_gallery.xml" />
	<!-- <include url="grid-settings.xml"/> -->

	<!-- This is the skin_settings where hs_anim_delay is set. -->
  <skin_settings maps="true"
	               maps_type=""
	               custom_map="false"	
				   			 menu-btn="true"
				         map-btn="false"
				         navxoffset="0"
				         vr-icon-xoffset="-106"
				  
				   			 hs_delay="1"
				   			 hs_anim_delay="6"
				   			 bg_type="stretch"
				   			 corporate_logo="true"
				   			 corp-logo-xoffset="10"
				   			 project_logo="true"
				   
				   			 zoom_buttons="false"	
				   			 speak_buttons="false"
				   			 password="false"
				   			 start_screen="false"
				   			 maps_bing_api_key=""
	               maps_zoombuttons="false"
				   			 maps_zoom="25"
				   			 share-btn="false"
	               gyro="true"
	               arrows_html="true"
				   			 title="1" 
				   			 grid_button="false"
				    		 littleplanetintro="true"
	               />

	<layer name="skin_logo" url="" scale="0.25" opened_onclick="openurl('...',_blank);" />
	
	<action name="startup">
		delayedcall(1, intro-page(););
	</action>

	<action name="intro-page">

		if(skin_settings.start_screen,
 	 		if(device.html, splash(); , flashsplash(); );,
 			set(layer[intro-con].visible,false);
			set(intro_done,true);
  		if(device.html, choose(); , flash-choose();); 
 			hs_int(); 
 	); 

</action>

