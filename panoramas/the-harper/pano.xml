<krpano version="1.19" title="The Harper Views" onstart="startup();" showerrors="false">
	
	<!-- the skin -->
	<include url="skin/vtourskin.xml" />
	<include url="skin/custom.xml" />

 	<skin_settings navxoffset="0"
		         vr-icon-xoffset="10"
		         fs-icon-xoffset="55"
	   			 hs_delay="1"
	   			 hs_anim_delay="0"
	   			 bg_type="stretch"
	   			 corporate_logo="true"
   			 	 corp-logo-xoffset="10"
           		 arrows_html="true"
   	/>
	
	<action name="startup">
		<!-- TODO: Add analytics on which floors are visited most, which directions are looked at most. -->
		set(scene_name, "floor_24");
		set(view.fov, 110);
		set(view.fovmax, 110);
		set(view.fovmin, 40);
		if(fl !== null,
			txtadd(scene_name, "floor_", get(fl));
		);
		set(layer[intro-con].visible,false);
		set(intro_done,true);
		loadscene(get(scene_name),null,get(skin_settings.loadscene_flags),get(skin_settings.loadscene_blend)); 

		if(dir !== null,
			if(dir == 'n',
				set(offset, 0);
			);
			if(dir == 'w',
				set(offset, -90);
			);
			if(dir == 's',
				set(offset, 180);
			);
			if(dir == 'e',
				set(offset, 90);
			);
			lookat(get(offset));
		);	
	</action>

	<!-- FLOOR 24 SCENE -->
	<scene name="floor_24" title="" lat="" lng="" heading="">

		<!-- TODO: Remove this to take out the logs. -->
		<events onclick="screentosphere(mouse.x, mouse.y, m_ath, m_atv); showlog(); trace('clicked at ath=',m_ath,' atv=',m_atv);" />
		
		<!-- view settings -->
		<view hlookat="58" vlookat="1.5" fovtype="MFOV" vlookatmax="-90" vlookatmin="90" limitview="range" />

		<preview url="panos/floor_24.tiles/preview.jpg" />
		<image type="CUBE" prealign="0|32.0|0" multires="true" tilesize="512">
			<level tiledimagewidth="8256" tiledimageheight="8256">
				<cube url="panos/floor_24.tiles/%s/l4/%0v/l4_%s_%0v_%0h.jpg" />
			</level>
			<level tiledimagewidth="4160" tiledimageheight="4160">
				<cube url="panos/floor_24.tiles/%s/l3/%0v/l3_%s_%0v_%0h.jpg" />
			</level>
			<level tiledimagewidth="2048" tiledimageheight="2048">
				<cube url="panos/floor_24.tiles/%s/l2/%0v/l2_%s_%0v_%0h.jpg" />
			</level>
			<level tiledimagewidth="1024" tiledimageheight="1024">
				<cube url="panos/floor_24.tiles/%s/l1/%0v/l1_%s_%0v_%0h.jpg" />
			</level>
		</image>

	</scene>

